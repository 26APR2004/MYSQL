CREATE DATABASE College;
CREATE DATABASE IF NOT EXISTS College;
USE College;
DROP DATABASE IF EXISTS Colni;
CREATE TABLE Student1(
  id INT PRIMARY KEY,
  name VARCHAR(50),
  age INT NOT NULL
);


USE College;

CREATE TABLE Class(
prn VARCHAR(10) PRIMARY KEY,
name VARCHAR(20) ,
cgpa INT NOT NULL,
city VARCHAR(10),
grade VARCHAR(1)
);

INSERT INTO Class(prn,name,cgpa,city,grade) VALUES 

("22UAI312","Parth Kishrasagar",8.5,"Sangli","A");


SELECT MAX(cgpa) FROM Class;
SELECT AVG(cgpa) FROM Class;
SELECT COUNT(prn) FROM Class;
SELECT MIN(cgpa) FROM Class;
SELECT SUM(cgpa) FROM Class;
